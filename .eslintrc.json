{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
      "linebreak-style": [
        "error",
        "windows"
      ],
      "react/prop-types": [
        0,
        { 
          "ignore": [],
          "customValidators": [],
          "skipUndeclared": true
        }
      ],
      "no-shadow": [
        "off",
        {
          "builtinGlobals": false,
          "hoist": "functions",
          "allow": []
        }
      ]
      
    }
}
